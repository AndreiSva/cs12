# Reversi test data

import reversi

board1 = [[" ", " ", " ", " ", " ", " ", " ", " "],
          ["○", " ", " ", " ", " ", " ", " ", " "],
          ["●", " ", " ", " ", " ", " ", " ", " "],
          ["●", " ", " ", "●", " ", " ", " ", " "],
          ["●", " ", "○", " ", " ", " ", " ", " "],
          ["●", "○", " ", " ", " ", " ", " ", " "],
          [" ", " ", " ", " ", " ", " ", " ", " "],
          [" ", "●", "●", "●", "●", "●", "●", "○"]]

board2 = [[' ', ' ', ' ', ' ', ' ', ' ', ' ', ' '],
          [' ', ' ', ' ', ' ', ' ', ' ', ' ', ' '],
          ['●', '●', '●', ' ', '○', '●', ' ', ' '],
          [' ', ' ', '○', '○', '○', '○', ' ', ' '],
          [' ', ' ', '●', '○', '○', ' ', ' ', ' '],
          [' ', ' ', ' ', ' ', '○', ' ', ' ', ' '],
          [' ', ' ', ' ', ' ', ' ', '○', ' ', ' '],
          [' ', ' ', ' ', ' ', ' ', ' ', ' ', ' ']]

assert max_moves(board1, "●") == [(0,6)]
assert max_moves(board1, "○") == [(0,7)]
assert minimax_moves(board1, "●") == [(0, 0), (2, 5), (2, 6)]
assert minimax_moves(board1, "○") == [(0,7)]

assert max_moves(board2, "●") == [(6,7)]
assert max_moves(board2, "○") == [(0, 1), (1, 1), (1, 4), (1, 5), (2, 1), (2, 5), (5, 1), (6, 1), (6, 2)]
assert minimax_moves(board2, "●") == [(6, 7)]
assert minimax_moves(board2, "○") == [(0, 1)]
